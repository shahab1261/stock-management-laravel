<script>
document.addEventListener("DOMContentLoaded", function () {
    // Get the current page URL path
    let currentPage = window.location.pathname;

    // Select all navigation links
    let navLinks = document.querySelectorAll("#navmenu a");

    navLinks.forEach(link => {
        let linkHref = link.getAttribute("href");

        // Mark individual product links as active
        if (linkHref === currentPage) {
            link.classList.add("active");
        }

        // If the current page is inside the "/products/" folder, highlight "Products"
        if (currentPage.startsWith("/products/")) {
            let productsMenuItem = document.querySelector("#navmenu .dropdown > a");
            if (productsMenuItem) {
                productsMenuItem.classList.add("active");
            }
        }
    });
});


</script>
